<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GetList</servlet-name>
		<servlet-class>ua.kiev.prog.GetListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetList</servlet-name>
		<url-pattern>/get</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Add</servlet-name>
		<servlet-class>ua.kiev.prog.AddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Add</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Registration</servlet-name>
		<servlet-class>ua.kiev.prog.RegistrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registration</servlet-name>
		<url-pattern>/reg</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>ua.kiev.prog.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetUsers</servlet-name>
		<servlet-class>ua.kiev.prog.GetUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetUsers</servlet-name>
		<url-pattern>/users</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Exit</servlet-name>
		<servlet-class>ua.kiev.prog.ExitServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Exit</servlet-name>
		<url-pattern>/exit</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserStatus</servlet-name>
		<servlet-class>ua.kiev.prog.UserStatusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserStatus</servlet-name>
		<url-pattern>/status</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CreateChatRoom</servlet-name>
		<servlet-class>ua.kiev.prog.CreateChatRoomServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateChatRoom</servlet-name>
		<url-pattern>/createroom</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EnterChatRoom</servlet-name>
		<servlet-class>ua.kiev.prog.EnterChatRoomServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EnterChatRoom</servlet-name>
		<url-pattern>/enterroom</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetCList</servlet-name>
		<servlet-class>ua.kiev.prog.GetCListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetCList</servlet-name>
		<url-pattern>/getrooms</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CheckParticipant</servlet-name>
		<servlet-class>ua.kiev.prog.CheckParticipantServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CheckParticipant</servlet-name>
		<url-pattern>/checkroom</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
